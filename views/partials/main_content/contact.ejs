<% if (title == "Contact"){ %>
    <!--Section: Contact v.1-->
    <section class="section pb-5">
    
      <!--Section heading-->
      <h1 class="text-center pt-4">Contact me</h1>
      <!--Section description-->
      <p class="text-center pb-4">Let's Collaborate!</p>
    
      <div class="row">
    
        <!--Grid column-->
        <div class="col-lg-5 mb-4">
    
          <!--Form with header-->
          <div class="card">
    
            <div class="card-body">
              <!--Header-->
              <div class="form-header accent-1">
                <h3><i class="fas fa-envelope"></i> Write:</h3>
              </div>
    
              
              <br>
    
              <!--Body-->
              <div class="md-form">
                <i class="fas fa-user prefix grey-text"></i>
                <input type="text" id="form-name" class="form-control" placeholder="Full Name">
                
              </div>
    
              <div class="md-form">
                <i class="fas fa-envelope prefix grey-text"></i>
                <input type="email" id="form-email" class="form-control" placeholder="Email">
                
              </div>
    
              <div class="md-form">
                <i class="fas fa-tag prefix grey-text"></i>
                <input type="text" id="form-Subject" class="form-control" placeholder="Subject">
                
              </div>
    
              <div class="md-form">
                <i class="fas fa-pencil-alt prefix grey-text"></i>
                <textarea id="form-text" class="form-control md-textarea" rows="3" placeholder="Message"></textarea>
                
              </div>
    
              <div class="text-center mt-4">
                <button class="btn btn-dark">Submit</button>
              </div>
    
            </div>
    
          </div>
          <!--Form with header-->
    
        </div>
        <!--Grid column-->
    
        <!--Grid column-->
        <div class="col-lg-7 text-center">
    
          <!--Google map-->
          <div id="map-container-google-9" class="z-depth-1-half map-container-5" style="height: 350px">
            <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d13786.753470254893!2d75.8434892!3d30.2459621!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x5d578d3ead2eac4d!2sDaffodils%20Punjabi%20Suits!5e0!3m2!1sen!2sin!4v1613329572575!5m2!1sen!2sin"
             width="600" height="320" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
          </div>
    
          
          <!--Buttons-->
          <div class="row text-center">
            <div class="col-md-4">
              <a class="btn-floating blue accent-1"><i class="fas fa-map-marker-alt"></i></a>
              <p class="pt-2">SCO 58, New Quilla Market,Sangrur,Punjab</p>
              <p>India</p>
            </div>
    
            <div class="col-md-4">
              <a class="btn-floating blue accent-1"><i class="fas fa-phone"></i></a>
              <p class="pt-2" >+ 91 8288804644</p>
              <p>Mon-Sat 9-5pm</p>
            </div>
    
            <div class="col-md-4">
              <a class="btn-floating blue accent-1"><i class="fas fa-envelope"></i></a>
              <p class="pt-2">sbindal1@mycentennialcollege.ca</p>
              <p class="pt-2">samridhibindal97@gmail.com</p>
              
            </div>
          </div>
    
        </div>
        <!--Grid column-->
    
      </div>
    
    </section>
    
        <% } %>